{
  "name": "edu-funds",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3101",
    "build": "next build",
    "start": "next start -p 3101",
    "lint": "next lint",
    "setup:db": "npx ts-node scripts/setup-db.ts",
    "db:backup": "./scripts/backup.sh",
    "docker:dev": "docker-compose up -d",
    "docker:dev:logs": "docker-compose logs -f nextjs",
    "docker:dev:stop": "docker-compose down",
    "docker:prod:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:prod:start": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:stop": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "health:check": "curl http://localhost:3101/api/health"
  },
  "dependencies": {
    "html2pdf.js": "^0.10.1",
    "lucide-react": "^0.474.0",
    "next": "^14.2.0",
    "pg": "^8.18.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "resend": "^4.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3"
  }
}
